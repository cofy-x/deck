{
  "name": "@cofy-x/client-daemon",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup && node ../../scripts/build_package.js",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "generate": "openapi --input ../daemon/pkg/toolbox/docs/swagger.json --output ./src --client fetch --name DaemonClient"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "^5.9.3",
    "tsup": "^8.5.1"
  }
}
