{
  "name": "@cofy-x/deck-core-ts",
  "version": "0.0.1",
  "private": true,
  "description": "Deck Core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cofy-x/deck.git"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && node ../../scripts/build_package.js",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "tsup": "^8.5.1"
  }
}
